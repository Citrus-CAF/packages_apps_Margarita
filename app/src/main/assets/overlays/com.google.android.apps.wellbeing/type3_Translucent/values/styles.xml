<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.wellbeing:style/Theme.GoogleMaterial.Dark">
        <item name="android:navigationBarColor">@*android:color/holo_primary</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:statusBarColor">@*android:color/holo_primary_dark</item>
        <item name="android:colorBackground">@*android:color/background_holo_dark</item>
        <item name="android:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.apps.wellbeing:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimary">@*android:color/holo_primary</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimaryDark">@*android:color/holo_primary_dark</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimaryVariant">@*android:color/holo_primary_dark</item>
        <item name="android:windowBackground">@*android:color/background_holo_dark</item>
        <item name="android:windowShowWallpaper">true</item>
    </style>

    <style name="Widget.GoogleMaterial.CardView.Dark" parent="@*com.google.android.apps.wellbeing:style/Widget.GoogleMaterial.CardView">
        <item name="*com.google.android.apps.wellbeing:strokeColor">@*android:color/background_holo_dark</item>
    </style>

    <style name="Platform.GoogleMaterial.Light.Bridge" parent="@*com.google.android.apps.wellbeing:style/Platform.GoogleMaterial.Dark.Bridge" >
        <item name="*com.google.android.apps.wellbeing:colorAccent">@android:color/holo_blue_bright</item>
        <item name="android:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimaryDark">@*android:color/holo_primary_dark</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimaryVariant">@*android:color/holo_primary_dark</item>
        <item name="android:colorBackground">@*android:color/background_holo_dark</item>
    </style>

    <style name="Platform.GoogleMaterial.Light.Dialog" parent="@*com.google.android.apps.wellbeing:style/Platform.GoogleMaterial.Dark.Dialog" >
        <item name="*com.google.android.apps.wellbeing:colorAccent">@android:color/holo_blue_bright</item>
        <item name="android:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimaryDark">@*android:color/holo_primary_dark</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimaryVariant">@*android:color/holo_primary_dark</item>
        <item name="android:colorBackground">@*android:color/background_holo_dark</item>
    </style>

    <style name="Widget.Wellbeing.ChevronButton" parent="@*com.google.android.apps.wellbeing:style/Widget.GoogleMaterial.Button.Icon">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">48.0dip</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:insetLeft">0.0dip</item>
        <item name="android:insetRight">0.0dip</item>
        <item name="android:insetTop">0.0dip</item>
        <item name="android:insetBottom">0.0dip</item>
        <item name="*com.google.android.apps.wellbeing:backgroundTint">@*android:color/holo_primary</item>
        <item name="*com.google.android.apps.wellbeing:cornerRadius">24.0dip</item>
        <item name="*com.google.android.apps.wellbeing:iconTint">@*android:color/holo_blue_bright</item>
        <item name="*com.google.android.apps.wellbeing:rippleColor">@*android:color/ripple_material_dark</item>
    </style>

    <style name="Widget.GoogleMaterial.Snackbar" parent="@*com.google.android.apps.wellbeing:style/Widget.MaterialComponents.Snackbar">
        <item name="android:background">@*android:color/holo_primary_dark</item>
    </style>

    <style name="Widget.GoogleMaterial.TimePicker" parent="@*android:style/Widget.Material.TimePicker">
        <item name="android:headerBackground">@*com.google.android.apps.wellbeing:drawable/google_picker_header_with_divider</item>
        <item name="android:numbersBackgroundColor">@*android:color/holo_primary_dark</item>
    </style>

    <style name="Theme.AppCompat.Light" parent="@*com.google.android.apps.wellbeing:style/Base.Theme.AppCompat">
        <item name="android:colorAccent">@android:color/holo_blue_bright</item>
        <item name="android:colorPrimary">@*android:color/holo_primary</item>
        <item name="android:colorPrimaryDark">@*android:color/holo_primary_dark</item>
        <item name="android:windowBackground">@*android:color/background_holo_dark</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="*com.google.android.apps.wellbeing:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimary">@*android:color/holo_primary</item>
        <item name="*com.google.android.apps.wellbeing:colorPrimaryDark">@*android:color/holo_primary_dark</item>
    </style>

</resources>
